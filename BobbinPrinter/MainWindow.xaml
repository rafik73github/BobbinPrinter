        <Button x:Name="AddPrintButton" Content="DRUKOWANIE ETYKIET" HorizontalAlignment="Center" VerticalAlignment="Top" Width="315" Margin="0,82,0,0" Height="30" FontSize="14" Click="AddPrintButton_Click"/>
        <Button x:Name="YarnListButton" Content="LISTA PRZĘDZ" HorizontalAlignment="Center" VerticalAlignment="Top" Width="315" Margin="0,132,0,0" Height="30" FontSize="14" Click="YarnListButton_Click"/>
        <Button x:Name="MainMenuExitButton" Content="ZAMKNIJ" HorizontalAlignment="Center" VerticalAlignment="Top" Width="315" Margin="0,332,0,0" Height="30" FontSize="14" Click="MainMenuExitButton_Click"/>
        <Label x:Name="label" Content="PANEL GŁÓWNY" HorizontalAlignment="Center" Height="30" Margin="17,24,17,0" VerticalAlignment="Top" Width="315" FontSize="16" HorizontalContentAlignment="Center" Foreground="White" FontWeight="Bold"/>

    </Grid>
</Window>
