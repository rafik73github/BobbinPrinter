<Window x:Class="BobbinPrinter.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BobbinPrinter"
        mc:Ignorable="d"
        Title="BobbinPrinter" Height="450" Width="355" ResizeMode="NoResize" Margin="0" Left="600" Top="200">
    <Grid>
        <Button x:Name="AddPrintButton" Content="DRUKOWANIE ETYKIET" HorizontalAlignment="Center" VerticalAlignment="Top" Width="315" Margin="17,82,17,0" Height="30" FontSize="14" Click="AddPrintButton_Click"/>
        <Button x:Name="TextillListButton" Content="LISTA PRZĘDZ" HorizontalAlignment="Center" VerticalAlignment="Top" Width="315" Margin="17,132,17,0" Height="30" FontSize="14" Click="TextillListButton_Click"/>
        <Button x:Name="MainMenuExitButton" Content="ZAMKNIJ" HorizontalAlignment="Center" VerticalAlignment="Top" Width="315" Margin="17,366,17,0" Height="30" FontSize="14" Click="MainMenuExitButton_Click"/>
        <Label x:Name="label" Content="PANEL GŁÓWNY" HorizontalAlignment="Center" Height="30" Margin="17,24,17,0" VerticalAlignment="Top" Width="315" FontSize="16" HorizontalContentAlignment="Center"/>

    </Grid>
</Window>
