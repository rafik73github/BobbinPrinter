<Window x:Class="BobbinPrinter.YarnList"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BobbinPrinter"
        mc:Ignorable="d"
        Title="YarnList" Height="640" Width="1200" Top="100" Left="100" ResizeMode="NoResize" WindowStyle="None" Background="#FF362236">
    <Grid Width="1192" Margin="0" Height="609" VerticalAlignment="Bottom">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="99*"/>
            <ColumnDefinition Width="99*"/>
            <ColumnDefinition Width="100*"/>
        </Grid.ColumnDefinitions>
        <GroupBox x:Name="groupBox1" Grid.Column="1" Header="DODAWANIE PRODUCENTA PRZĘDZY" HorizontalAlignment="Center" Height="131" Margin="0,10,0,0" VerticalAlignment="Top" Width="375" FontWeight="Bold" FontSize="14" Foreground="#FFFFE800"/>
        <GroupBox x:Name="groupBox2" Header="LISTA PRZĘDZ" HorizontalAlignment="Left" Height="568" Margin="10,10,0,0" VerticalAlignment="Top" Width="377" FontSize="14" FontWeight="Bold" Foreground="#FFFFE800"/>
        <GroupBox x:Name="groupBox" Grid.Column="2" Header="DODAWANIE PRZĘDZ" HorizontalAlignment="Center" Height="266" Margin="0,10,0,0" VerticalAlignment="Top" Width="375" Foreground="#FFFFE800" BorderBrush="White" FontSize="14" FontWeight="Bold"/>
        <ComboBox x:Name="FilterYarnComboBox" HorizontalAlignment="Center" Height="30" Margin="32,39,20,0" VerticalAlignment="Top" Width="344"/>
        <ComboBox x:Name="SelectYarnMakerComboBox" HorizontalAlignment="Center" Height="30" Margin="0,39,0,0" VerticalAlignment="Top" Width="348" Grid.Column="2"  VerticalContentAlignment="Center" FontSize="14" ToolTip="wybierz producenta przędzy"/>
        <TextBox x:Name="AddColorNameTextBox" Grid.Column="2" HorizontalAlignment="Center" Height="30" Margin="0,172,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="348" FontSize="14" VerticalContentAlignment="Center" ToolTip="wpisz kolor przędzy"/>
        <TextBox x:Name="AddMakerTextBox" Grid.Column="1" HorizontalAlignment="Center" Height="30" Margin="0,39,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="348" FontSize="14" VerticalContentAlignment="Center" ToolTip="wpisz producenta przędzy (np: FRAFIL)"/>
        <Button x:Name="AddYarnButton" Content="DODAJ PRZĘDZĘ" Grid.Column="2" HorizontalAlignment="Center" Height="30" Margin="0,224,0,0" VerticalAlignment="Top" Width="348" FontSize="14" Click="AddYarnButton_Click"/>
        <Button x:Name="AddMakerButton" Content="DODAJ PRODUCENTA" Grid.Column="1" HorizontalAlignment="Center" Height="30" Margin="0,92,0,0" VerticalAlignment="Top" Width="348" FontSize="14" Click="AddMakerButton_Click"/>
        <Button x:Name="BackToMainMenuButton" Content="WRÓC DO GŁÓWNEGO PANELU" Grid.Column="2" HorizontalAlignment="Center" Height="30" Margin="25,483,27,0" VerticalAlignment="Top" Width="348" FontSize="14" Click="BackToMainMenuButton_Click"/>
        <Button x:Name="YarnListExitProgramButton" Content="ZAKOŃCZ PROGRAM" Grid.Column="2" HorizontalAlignment="Center" Height="30" Margin="25,530,27,0" VerticalAlignment="Top" Width="348" FontSize="14" Click="YarnListExitProgramButton_Click"/>
        <ComboBox x:Name="SelectYarnTypeComboBox" HorizontalAlignment="Center" Height="30" Margin="0,83,0,0" VerticalAlignment="Top" Width="348" Grid.Column="2"  VerticalContentAlignment="Center" FontSize="14" ToolTip="wybierz typ przędzy"/>
        <ComboBox x:Name="SelectYarnSizeComboBox" HorizontalAlignment="Center" Height="30" Margin="0,128,0,0" VerticalAlignment="Top" Width="348" Grid.Column="2"  VerticalContentAlignment="Center" FontSize="14" ToolTip="wybierz grubość przędzy"/>
        <GroupBox x:Name="groupBox1_Copy" Grid.Column="1" Header="DODAWANIE TYPU PRZĘDZY" HorizontalAlignment="Center" Height="131" Margin="0,147,0,0" VerticalAlignment="Top" Width="375" FontWeight="Bold" FontSize="14" Foreground="#FFFFE800"/>
        <GroupBox x:Name="groupBox1_Copy1" Grid.Column="1" Header="DODAWANIE GRUBOŚCI PRZĘDZY" HorizontalAlignment="Center" Height="131" Margin="0,283,0,0" VerticalAlignment="Top" Width="375" FontWeight="Bold" FontSize="14" Foreground="#FFFFE800"/>
        <TextBox x:Name="AddTypeTextBox" Grid.Column="1" HorizontalAlignment="Center" Height="30" Margin="0,175,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="348" FontSize="14" VerticalContentAlignment="Center" ToolTip="wpisz typ przędzy (np: AB 50/50)"/>
        <Button x:Name="AddTypeButton" Content="DODAJ TYP" Grid.Column="1" HorizontalAlignment="Center" Height="30" Margin="0,229,0,0" VerticalAlignment="Top" Width="348" FontSize="14" Click="AddTypeButton_Click" />
        <TextBox x:Name="AddSizeTextBox" Grid.Column="1" HorizontalAlignment="Center" Height="30" Margin="0,309,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="348" FontSize="14" VerticalContentAlignment="Center" ToolTip="wpisz grubość przędzy (np: 32/2)"/>
        <Button x:Name="AddSizeButton" Content="DODAJ GRUBOŚĆ" Grid.Column="1" HorizontalAlignment="Center" Height="30" Margin="0,362,0,0" VerticalAlignment="Top" Width="348" FontSize="14" Click="AddSizeButton_Click" />
    </Grid>
</Window>
